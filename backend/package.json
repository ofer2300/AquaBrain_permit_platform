{
  "name": "building-permit-platform-backend",
  "version": "1.0.0",
  "description": "Backend API for AI-powered building permit compliance platform",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:integration": "jest --testPathPattern=integration",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "migrate": "ts-node src/database/migrations/run.ts",
    "seed": "ts-node src/database/seeds/run.ts"
  },
  "keywords": [
    "building-permit",
    "compliance",
    "api",
    "express",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.3",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "dotenv": "^16.4.5",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "pg": "^8.11.5",
    "redis": "^4.6.13",
    "multer": "^1.4.5-lts.1",
    "express-validator": "^7.0.1",
    "express-rate-limit": "^7.2.0",
    "morgan": "^1.10.0",
    "winston": "^3.12.0",
    "axios": "^1.6.8",
    "uuid": "^9.0.1",
    "date-fns": "^3.6.0",
    "ioredis": "^5.3.2",
    "bull": "^4.12.9",
    "nodemailer": "^6.9.13",
    "pdfkit": "^0.14.0",
    "sharp": "^0.33.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.5",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/pg": "^8.11.4",
    "@types/multer": "^1.4.11",
    "@types/morgan": "^1.9.9",
    "@types/uuid": "^9.0.8",
    "@types/nodemailer": "^6.4.14",
    "@types/pdfkit": "^0.13.4",
    "@types/jest": "^29.5.12",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5",
    "ts-node": "^10.9.2",
    "nodemon": "^3.1.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "supertest": "^6.3.4",
    "@jest/globals": "^29.7.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
